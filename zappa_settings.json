{
  "common": {
    "project_name": "the-spymaster-backend",
    "profile_name": "default",
    "aws_region": "us-east-1",
    "django_settings": "the_spymaster.settings",
    "runtime": "python3.8",
    "exclude": [
      "botocore",
      "tf_secrets*",
      "tf_service*",
      "logs",
      "node_modules",
      "language_data",
      ".mypy_cache",
      "mypy",
      "mypyc",
      "*.sqlite3",
      "local.toml",
      "secrets.toml"
    ],
    "timeout_seconds": 60,
    "lambda_concurrency": 5,
    "memory_size": 1000,
    "keep_warm": false,
    "manage_roles": false
  },
  "dev": {
    "extends": "common",
    "s3_bucket": "zappa-deployment-bucket-dev",
    "role_arn": "arn:aws:iam::${local.aws_account_id}:role/the-spymaster-backend-dev-exec-role",
    "aws_environment_variables": {
      "ENV_FOR_DYNACONF": "dev"
    }
  }
}
