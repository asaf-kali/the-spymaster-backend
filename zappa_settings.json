{
  "common": {
    "aws_region": "us-east-1",
    "profile_name": "default",
    "django_settings": "the_spymaster.settings",
    "project_name": "the-spymaster",
    "runtime": "python3.8",
    "exclude": [
      "logs",
      "node_modules",
      "language_data",
      "*.sqlite3",
      "local.toml",
      "secrets.toml",
      ".mypy_cache"
    ],
    "exclude_expressions": [
      "^boto3.*",
      "^botocore.*",
      "^scipy.*",
      "^pydantic.*",
      "^gensim.*",
      "^pandas.*",
      "^numpy.*",
      "^scipy.*",
      "^networkx.*",
      "^cryptography.*",
      "^mypy.*",
      "^virtualenv.*",
      "^selenium.*"
    ],
    "callbacks": {
      "zip": "deployment.zappa_package_cleaner.main"
    },
    "slim_handler": false,
    "timeout_seconds": 60,
    "lambda_concurrency": 3,
    "memory_size": 2000
  },
  "dev": {
    "extends": "common",
    "s3_bucket": "zappa-deployment-bucket-dev",
    "aws_environment_variables": {
      "ENV_FOR_DYNACONF": "dev"
    }
  },
  "stage": {
    "extends": "common",
    "s3_bucket": "zappa-deployment-bucket-stage",
    "aws_environment_variables": {
      "ENV_FOR_DYNACONF": "stage"
    }
  }
}
